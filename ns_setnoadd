#include "module.h"

class CommandNSSetNoop : public Command
{
 public:
	CommandNSSetNoop(Module *creator) : Command(creator, "nickserv/set/noop", 3, 3)
	{
		this->SetDesc(_("Set ability to be added in channel access list"));
		this->SetSyntax(_("\037ON/OFF\037 \037Status\037 \037description\037"));
	}
